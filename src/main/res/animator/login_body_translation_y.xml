<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">
    <set>
        <objectAnimator
            android:propertyName="scaleX"
            android:valueFrom="0"
            android:valueTo="1.0"
            android:startOffset="35"
            android:duration="0" />
        <objectAnimator
            android:propertyName="scaleY"
            android:valueFrom="0"
            android:valueTo="1.0"
            android:startOffset="35"
            android:duration="0" />
        <objectAnimator
            android:propertyName="translateY"
            android:valueFrom="202"
            android:valueTo="102"
            android:startOffset="35"
            android:duration="500" />
    </set>
    <objectAnimator
        android:propertyName="translateY"
        android:valueFrom="102"
        android:valueTo="135"
        android:duration="500" />
</set>



