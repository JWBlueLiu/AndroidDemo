        <!--<path-->
        <!--android:fillColor="@android:color/holo_red_light"-->
        <!--android:pathData="M 0,50 Q 50,0 100,50 Q 50,100 0,50 z" />-->
    </group>
    <group android:name="eye">
        <!-- 眼珠 -->
        <path
            android:name="eye"
            android:tintMode="multiply"
            android:fillColor="@android:color/black"
            android:pathData="M 40,50 a 10,5 0 0 1 20,0 a 10,5 0 0 1 -20,0 z" />
    </group>
</vector>
